{
  "name": "dvdlogobounce",
  "version": "1.0.0",
  "description": "DVD logo bounce",
  "main": "index.js",
  "scripts": {
    "serve": "live-server public/",
    "start": "serve -s build",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "wasm": "emcc ./src/wasm/movement.c -s EXPORT_ES6=1 -s USE_ES6_IMPORT_META=0 -s MODULARIZE=1 -s EXPORTED_FUNCTIONS=\"['_get_canvas_size', '_get_selection','_change_direction','_update_axis','_get_canvas_size']\" -o ./src/wasm/movement_wasm.js -O3",
    "build": "webpack --config webpack.prod.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "istareatscreens",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "style-loader": "^1.3.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "serve": "^11.3.2"
  }
}